{
  "manifest_version": 3,
  "name": "AUPOS Overlay",
  "version": "1.0.0",
  "description": "AI Productivity Overlay for SaaS platforms",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
